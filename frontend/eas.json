{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
      "buildType": "apk",
      "env": {
          "ANDROID_COMPILE_SDK_VERSION": "33"
        }
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android":{
        "buildType": "app-bundle",
         "withoutCredentials": true,
        "image": "default",
        "env": {
          "ANDROID_COMPILE_SDK_VERSION": "33"
        }
         
       
      }
    }
  },
  "submit": {
    "production": {}
  }
}
